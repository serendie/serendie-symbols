{
  "name": "@serendie/symbols",
  "description": "300+ SVG icons as part of Serendie Design System by Mitsubishi Electric",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/symbols.js",
  "module": "dist/symbols.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "generate-symbol-names": "node scripts/generateSymbolNames.js",
    "replace-components-index": "node scripts/replaceComponentsIndex.js",
    "generate:tsx": "rimraf src/generated/components && svgr ./assets/outlined --out-dir ./src/generated/components/outlined --typescript && svgr ./assets/filled --out-dir ./src/generated/components/filled --typescript",
    "generate": "npm run generate-symbol-names && npm run generate:tsx && npm run replace-components-index",
    "build": "vite build",
    "release": "release-it"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "release-it": "^17.10.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
